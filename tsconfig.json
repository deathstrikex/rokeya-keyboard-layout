{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "ESNext",
        "noImplicitAny": true,
        "jsx": "react",
        "allowJs": true,
        "sourceMap": true,
        "esModuleInterop": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}